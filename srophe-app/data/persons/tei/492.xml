                        <addName type="family" sort="2">bar Bokhtishoʿ</addName>
                    </persName>
                    <persName xml:id="name492-2" corresp="#name492-3" xml:lang="syr" source="#bib492-6 #bib492-7" syriaca-tags="#syriaca-headword">
