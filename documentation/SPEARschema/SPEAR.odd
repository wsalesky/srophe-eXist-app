                <biblScope unit="volume">
                  <title level="s">SPEAR: Syriac Persons, Events, and Relations</title>
                  <idno type="URI">http://syriaca.org/spear</idno>
                </biblScope>
              </seriesStmt>
            </egXML>
          </exemplum>
          <!-- remarks needed -->
        </elementSpec>
        
